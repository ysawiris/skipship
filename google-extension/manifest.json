{
    "name": "ShipSkip Extention",
    "version": "1.0",
    "description": "Chrome Extention for SkipShip which allows you to team up to avoid minimum charge for free shipping",
    "permissions": ["activeTab", "storage", "tabs", "http://*/", "https://*/"],
    "author": "Youssef Sawiris",
    "browser_action": { "default_popup": "popup.html" },
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version": 2
}

